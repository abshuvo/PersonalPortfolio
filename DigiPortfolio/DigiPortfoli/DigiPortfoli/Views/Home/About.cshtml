    </div>
</section><!-- End About Section -->
</main>



<script src="https://code.jquery.com/jquery-3.2.1.min.js"
        integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
        crossorigin="anonymous"></script>
<script>

    $(document).ready(function () {
        getAboutPageData();
    });



    // Get all Category to display
    function getAboutPageData() {
        // Call Web API to get a list of Category
        $.ajax({
            url: '../Home/GetAboutPageData/',
            type: 'GET',
            dataType: 'json',
            success: function (aboutpagedata) {
                debugger
                $('#Description').append(aboutpagedata.Description);
                $('#Name').append(aboutpagedata.Name);
                $('#Birthdate').append(aboutpagedata.Birthdate);
                $('#Profession').append(aboutpagedata.Profession);
                $('#Address').append(aboutpagedata.Address);
                $('#Qualification').append(aboutpagedata.Qualification);
                $('#Mobile').append(aboutpagedata.Mobile);
                $('#Email').append(aboutpagedata.Email);
                $('#Age').append(aboutpagedata.Age);

                //var profession = " <span class='typed' >" + aboutpagedata.Profession + "</span>"
                //$('#profession').append(profession);
        
            },
            error: function (request, message, error) {
                debugger
                alert(message);
            }
        });
    }
</script>