        <div class="section-title">
            <h2>About</h2>
                <p id="Description">
 
                </p>
        </div>

        <div class="row">
                <div class="col-lg-4">
                    <img src="~/content/assets/img/profile-img.jpg" class="img-fluid" alt="" />
                @*<img src="~/Content/assets/img/profile-img.jpg" >*@
            </div>
            <div class="col-lg-8 pt-4 pt-lg-0 content">
                <h3>ASP .NET Developer</h3>
               @* <p class="fst-italic">
                    Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore
                    magna aliqua.
                </p>*@
                <div class="row">
                    <div class="col-lg-6">
                        <ul>
                                <li><i class="bi bi-chevron-right"></i> <strong>Name:</strong> <span id="Name"></span></li>
                                <li><i class="bi bi-chevron-right"></i> <strong>Birthdate:</strong> <span id="Birthdate"></span></li>
                                <li><i class="bi bi-chevron-right"></i> <strong>Profession:</strong> <span id="Profession"></span></li>
                                <li><i class="bi bi-chevron-right"></i> <strong>Address:</strong> <span id="Address"></span></li>
                               
                        </ul>
                    </div>
                    <div class="col-lg-6">
                        <ul>
                                <li><i class="bi bi-chevron-right"></i> <strong>Qualification:</strong> <span id="Qualification"></span></li>
